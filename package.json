{
  "name": "@blocklet/form-collector",
  "private": true,
  "scripts": {
    "dev": "vite --port $BLOCKLET_PORT",
    "build": "tsc && rm -rf dist .blocklet && vite build",
    "bundle": "npm run build && blocklet bundle --zip --create-release",
    "deploy": "npm run bundle && blocklet deploy .blocklet/bundle",
    "upload": "npm run bundle && blocklet upload .blocklet/release/blocklet.json",
    "bump": "blocklet version patch",
    "preview": "vite preview"
  },
  "dependencies": {
    "@formily/antd": "^2.2.12",
    "@formily/core": "^2.2.12",
    "@formily/react": "^2.2.12",
    "antd": "^4.24.2",
    "axios": "^1.1.3",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-use": "^17.4.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.9",
    "@vitejs/plugin-react": "^2.2.0",
    "fs-extra": "^10.1.0",
    "js-yaml": "^4.1.0",
    "jsdom": "^20.0.2",
    "less": "^4.1.3",
    "typescript": "^4.8.4",
    "vite": "^3.2.3",
    "vite-plugin-blocklet": "^0.4.81",
    "vite-plugin-html": "^3.2.0"
  }
}
